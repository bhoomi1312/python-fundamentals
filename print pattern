n=int(input("enter rows - "))
i=1
while(i<=n):
    j=1
    while(j<=n):
        print("*",end=" ")
        j=j+1
    print("\n")
    i=i+1
    
    
    enter rows - 7
* * * * * * * 

* * * * * * * 

* * * * * * * 

* * * * * * * 

* * * * * * * 

* * * * * * * 

* * * * * * * 
----------------------------------------------------
enter n - 4
